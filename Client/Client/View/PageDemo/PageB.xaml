<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Client.View.PageDemo.PageB">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="OrderNo:" />
            <Editor Text="{Binding OrderNo}" />
            <Label Text="Qty:" />
            <Editor Text="{Binding Qty}" />
            <Label Text="Price:" />
            <Editor Text="{Binding Price}" />
            <Label Text="DeadLine:" />
            <Editor Text="{Binding DeadLine}" />

            <Label Text="------------" />
            <Label Text="{Binding OrderNo}" />
            <Label Text="{Binding Qty}" />
            <Label Text="{Binding Price}" />
            <Label Text="{Binding DeadLine}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>